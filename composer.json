{
    "name": "identity-layer/jose",
    "description": "Core Library for the Identity Layer project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alvin Chevolleaux",
            "email": "alvin@chevolleaux.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-intl": "*",
        "ext-json": "^7.4",
        "ext-openssl": "^7.4",
        "alvinchevolleaux/collection": "^0.1",
        "myclabs/php-enum": "^1.7",
        "paragonie/constant_time_encoding": "^2.3",
        "symfony/polyfill-mbstring": "^1.17"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "IdentityLayer\\Jose\\": "src/Jose"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IdentityLayer\\Tests\\Jose\\": "tests/Jose"
        }
    }
}
